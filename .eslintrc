{
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "globals": {
    "document": false
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "node": true
  },
  "rules": {
    "no-duplicate-imports": 0,
    "import/no-duplicates": 2,
    "import/named": 0,
    "import/no-unresolved": 0,
    "react/prop-types": [
      1,
      {
        "skipUndeclared": true
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "tabWidth": 2,
        "printWidth": 80,
        "singleQuote": true,
        "jsxBracketSameLine": true,
        "trailingComma": "es5"
      }
    ]
  }
}
